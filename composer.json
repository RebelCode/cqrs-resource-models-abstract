{
    "name": "rebelcode/cqrs-resource-models-abstract",
    "description": "Abstract functionality for a CQRS approach to resource models.",
    "type": "library",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "RebelCode",
            "email": "dev@rebelcode.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^5.4 | ^7.0",
        "dhii/config-interface": "dev-release/0.1",
        "dhii/i18n-helper-base": "dev-develop",
        "dhii/expression-interface": "^0.2",
        "dhii/output-renderer-interface": "^0.3",
        "dhii/sql-interface": "^0.1",
        "dhii/invocable-interface": "0.1-alpha1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "ptrofimov/xpmock": "^1.1",
        "dhii/php-cs-fixer-config": "^0.1",
        "codeclimate/php-test-reporter": "<=0.3.2",
        "dhii/stringable-interface": "^0.1"
    },
    "autoload": {
        "psr-4": {
            "RebelCode\\Storage\\Resource\\": "src"
        }
